generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model genres {
  id   String @id @unique @default(uuid()) @db.Uuid
  name String @db.VarChar(255)
}

model ingredients {
  id   String @id @unique @default(uuid()) @db.Uuid
  name String @db.VarChar(255)
  pic  String @db.VarChar(255)
}
